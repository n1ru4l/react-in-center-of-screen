{
  "name": "@n1ru4l/react-in-center-of-screen",
  "version": "1.0.0-alpha.4",
  "description": "Utility components for checking if content is in the center of the viewport",
  "author": "Laurin Quast <laurinquast@googlemail.com>",
  "license": "MIT",
  "module": "./lib/index.js",
  "main": "./lib/bundle.umd.js",
  "files": [
    "lib",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "test": "jest",
    "prepublish": "yarn build",
    "build": "rimraf lib && babel src --no-babelrc --out-dir lib --copy-files --presets=@babel/preset-react --plugins=@babel/plugin-transform-flow-strip-types && cpy ./src/index.js ./lib --rename=index.js.flow && rollup -c",
    "lint": "eslint --ignore-path .gitignore **/*.js",
    "prettier-check": "prettier-check --ignore-path .gitignore '**/*.{js,md,json}'",
    "prettier-fix": "prettier --ignore-path .gitignore --write '**/*.{js,md,json}'"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-transform-flow-comments": "7.2.0",
    "@babel/plugin-transform-flow-strip-types": "7.2.3",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "cpy-cli": "2.0.0",
    "eslint": "5.13.0",
    "eslint-config-prettier": "3.6.0",
    "eslint-plugin-flowtype": "3.4.1",
    "eslint-plugin-jest": "22.3.0",
    "eslint-plugin-react": "7.12.4",
    "flow-bin": "0.93.0",
    "flow-typed": "2.5.1",
    "jest": "23.6.0",
    "lint-staged": "8.1.4",
    "lodash.debounce": "4.0.8",
    "lolex": "3.1.0",
    "prettier": "1.16.4",
    "prettier-check": "2.0.0",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "react-testing-library": "5.8.0",
    "rimraf": "2.6.3",
    "rollup": "0.68.2",
    "rollup-plugin-babel": "4.3.2"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.4.1"
  }
}
