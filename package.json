{
  "name": "@n1ru4l/react-in-center-of-screen",
  "version": "1.0.0-alpha.4",
  "description": "Utility components for checking if content is in the center of the viewport",
  "author": "Laurin Quast <laurinquast@googlemail.com>",
  "license": "MIT",
  "module": "./lib/index.js",
  "main": "./lib/bundle.umd.js",
  "files": [
    "lib",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "test": "jest",
    "prepublish": "yarn build",
    "build": "rimraf lib && babel src --no-babelrc --out-dir lib --copy-files --presets=@babel/preset-react --plugins=@babel/plugin-transform-flow-strip-types && cpy ./src/index.js ./lib --rename=index.js.flow && rollup -c",
    "lint": "eslint --ignore-path .gitignore **/*.js",
    "prettier-check": "prettier-check --ignore-path .gitignore '**/*.{js,md,json}'",
    "prettier-fix": "prettier --ignore-path .gitignore --write '**/*.{js,md,json}'"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-transform-flow-comments": "7.8.3",
    "@babel/plugin-transform-flow-strip-types": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-react": "7.8.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "23.6.0",
    "cpy-cli": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-flowtype": "3.13.0",
    "eslint-plugin-jest": "22.21.0",
    "eslint-plugin-react": "7.19.0",
    "flow-bin": "0.120.1",
    "flow-typed": "2.6.2",
    "jest": "23.6.0",
    "lint-staged": "8.2.1",
    "lodash.debounce": "4.0.8",
    "lolex": "4.2.0",
    "prettier": "1.19.1",
    "prettier-check": "2.0.0",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-testing-library": "5.9.0",
    "rimraf": "3.0.2",
    "rollup": "0.68.2",
    "rollup-plugin-babel": "4.4.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.4.1"
  }
}
